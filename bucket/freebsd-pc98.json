  "installer": {
    "script": "Write-Host \"NOP\"",
    "keep": "true"
  },
  "post_install": [
    "Get-ChildItem ${dir}\\*.bin | Rename-Item -NewName { $_.Name -replace '.bin','.tgz' }",
    "$readme98 = \"${dir}\\98readme\"",
    "$null = New-Item -ItemType Directory -Force -Path ${readme98}",
    "Move-Item -Path $dir\\*.txt -Destination $readme98",
    "$floppy98 = \"${dir}\\floppy98\"",
    "$null = New-Item -ItemType Directory -Force -Path ${floppy98}",
    "Move-Item -Path $dir\\floppy98-CHECKSUM.MD5 -Destination ${floppy98}\\CHECKSUM.MD5",
    "Move-Item -Path $dir\\*.flp -Destination $floppy98",
    "$bin98 = \"${dir}\\98bin\"",
    "$null = New-Item -ItemType Directory -Force -Path ${bin98}",
    "Move-Item -Path $dir\\98bin-CHECKSUM.MD5 -Destination ${bin98}\\CHECKSUM.MD5",
    "Move-Item -Path $dir\\98bin.* -Destination $bin98",
    "Move-Item -Path $dir\\*.tgz -Destination $bin98",
    "Move-Item -Path $dir\\install.sh -Destination $bin98",
    "$pc98 = \"${scoopdir}\\persist\\machine-pc98\\pc98\\\"",
    "$pc98bin = \"${pc98}\\98bin\\\"",
    "$null = New-Item -ItemType Directory -Force -Path $pc98bin",
    "Copy-Item -Force -Path \"${dir}\\*.exe\" -Destination $pc98bin"
  ]
